{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "no-unused-vars": ["error", {
            "argsIgnorePattern": "^_"
        }],
        "object-curly-newline": ["error", {
            "ObjectPattern": { "multiline": true }
        }]
    }
}
