// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Animal {
  id         Int    @id @default(autoincrement())
  name       String
  age        Int
  specie     String
  breed      String
  owner_name String
  owner_tel  String
}
