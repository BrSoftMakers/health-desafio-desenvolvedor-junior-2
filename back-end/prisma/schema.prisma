// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model owners {
  id          Int    @id @default(autoincrement())
  name        String
  phoneNumber String
  CPF         String @unique @db.VarChar(11)

  @@map("Owner")
}

enum Type {
  GATO
  CACHORRO
}

model pets {
  id    Int    @id @default(autoincrement())
  name  String
  age   Int
  type  Type
  breed String

  @@map("Pet")
}
