// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://admin:admin@localhost:5432"
}

model Animal {
  id Int @id @default(autoincrement())
  nome String
  idade Int
  tipo String
  raca String
  nomeDono String
  telefoneDono String
}